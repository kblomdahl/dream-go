{
    "batch_size": 2816,
    "num_unrolls": 8,
    "data": [
        "data/foxwq_big.sgf"
    ],

    "model": {
        "embeddings_size": 722,
        "repr": {
            "num_blocks": 15,
            "num_channels": 192
        },
        "dyn": {
            "num_blocks": 6,
            "num_channels": 128
        }
    },

    "loss": {
        "coefficients": {
            "policy": 1.0,
            "value": 1.0,
            "ownership": 0.1,
            "similarity": 0.1
        },

        "label_smoothing": 0.1,
        "discount_factor": 0.97
    },

    "optimizer": {
        "learning_rate": 3e-4,
        "decay_rate": 0.97,
        "weight_decay": 1e-5,
        "clipnorm": 5.0,
        "epochs": 300
    },

    "early_stopping": {
        "warmup_steps": 5000,
        "samples": 50,
        "max_slope": -1e-7
    }
}